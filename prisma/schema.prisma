generator client {
  provider = "prisma-client-py"
}


datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Supplier {
  id          String   @id @default(cuid())
  url         String   @unique
  riskScore   String
  summary     String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
